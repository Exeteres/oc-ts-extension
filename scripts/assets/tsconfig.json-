{
    "compilerOptions": {
        "target": "esnext",
        "outDir": "dist",
        "module": "commonjs",
        "lib": ["esnext"],
        "strict": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "rootDir": "src",
        "types": ["typescript-to-lua/language-extensions", "lua-types/jit"]
    },
    "tstl": {
        "luaTarget": "JIT"
    }
}
